{
  "name": "mac-ai-healthcare-backend",
  "version": "1.0.0",
  "description": "Backend API for MAC AI Healthcare Chatbot - Team JARVIS SIH 2025",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "setup-db": "node scripts/setupDatabase.js",
    "seed-data": "node scripts/seedData.js",
    "build": "echo 'No build step required for Node.js'",
    "deploy": "npm run test && npm run lint && node deploy.js"
  },
  "keywords": [
    "healthcare",
    "chatbot",
    "AI",
    "nodejs",
    "express",
    "mongodb",
    "SIH2025",
    "ayushman-bharat",
    "medical-ai",
    "emergency-services",
    "telemedicine"
  ],
  "author": {
    "name": "Team JARVIS",
    "email": "team.jarvis@sih2025.in",
    "url": "https://github.com/team-jarvis"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/team-jarvis/mac-ai-healthcare"
  },
  "bugs": {
    "url": "https://github.com/team-jarvis/mac-ai-healthcare/issues"
  },
  "homepage": "https://mac-ai-healthcare.com",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "socket.io": "^4.7.2",
    "axios": "^1.5.0",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.40.0",
    "nodemailer": "^6.9.4",
    "twilio": "^4.15.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "crypto": "^1.0.1",
    "path": "^0.12.7",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^3.0.2",
    "@types/node": "^20.5.0",
    "concurrently": "^8.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "semi": ["error", "always"],
      "quotes": ["error", "single"]
    }
  }
}